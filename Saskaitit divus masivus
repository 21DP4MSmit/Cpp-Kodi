#include <iostream>
int main() {
    
    int Masivs1[10];
    int Masivs2[10];
    int c = 0;
    
    std::cout << "1. Mas카va:" << std::endl;
    for(int i = 0; i < 10; i++) {
        std::cin >> Masivs1[i];
}
    std::cout << "2. Mas카va:" << std::endl;
    for(int i = 0; i < 10; i++) {
        std::cin >> Masivs2[i];
}
    std::cout << "Mas카vu reizin훮jumi:" << std::endl;
    for(int i = 0; i < 10; i++) {
        c = Masivs1[i] + Masivs2[i];
        std::cout << c << std::endl;
    }
}
