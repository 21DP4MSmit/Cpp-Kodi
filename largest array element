// Online C++ compiler to run C++ program online
#include <algorithm>
#include <iostream>

double largest_element(double arr[]){
    int temp;
    for(int i = 0; i < 5; i++) {
        if (arr[i] > temp){
            temp = arr[i];
        }
    }
    return temp;
}

int main() {
    double masivs[5];
    
    std::cout << "Masīva skaitļi:" << std::endl;
    for(int i = 0; i < 5; i++) {
        std::cin >> masivs[i];
    }
    std::cout << "Lielākais elements: " << std::endl;
    std::cout << largest_element(masivs);
}
