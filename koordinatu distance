// Online C++ compiler to run C++ program online
#include <iostream>
#include <cmath>

double length(int x1, int y1, int x2, int y2){
    return sqrt(pow((x2 - x1), 2) + pow((y2 - y1), 2) );
    }

int main() {
    int x1 = 0;
    int y1 = 0;
    int x2 = 0;
    int y2 = 0;
    
    std::cout << "Ievadiet x1: " << std::endl;
    std::cin >> x1;
    std::cout << "Ievadiet y1: " << std::endl;
    std::cin >> y1;
    std::cout << "Ievadiet x2: " << std::endl;
    std::cin >> x2;
    std::cout << "Ievadiet y2: " << std::endl;
    std::cin >> y2;
    
    std::cout << "x1 ir: " << x1 << std::endl;
    std::cout << "y1 ir: " << y1 << std::endl;
    std::cout << "x2 ir: " << x2 << std::endl;
    std::cout << "y2 ir: " << y2 << std::endl;

    std::cout << "Distance ir: " <<length(x1, y1, x2, y2);
}
