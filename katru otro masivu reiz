#include <iostream>

int main() {
    
    int Masivs[10], c = 0;
    
    std::cout << "Masīva skaitļi:" << std::endl;
    for(int i = 0; i < 10; i++) {
        std::cin >> Masivs[i];
}
    std::cout << "Masīva Reizinājumi:" << std::endl;
    for(int i = 0; i < 10; i++) {
        c = Masivs[i] * Masivs[i+1];
        i = i + 1;
        std::cout << c << std::endl;
}
}
